{
    "name": "pedro-santiago/laravel-activity-feed",
    "description": "A robust Laravel package for creating activity feeds with dynamic entity resolution and multiple relationships",
    "keywords": [
        "laravel",
        "activity",
        "feed",
        "timeline",
        "stream"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Santiago",
            "email": "contato@pedrosantiago.com.br"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PedroSantiago\\ActivityFeed\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PedroSantiago\\ActivityFeed\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PedroSantiago\\ActivityFeed\\ActivityFeedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
